<h2 class="d-flex justify-content-center">@Title</h2>
<div class="d-flex justify-content-center">
    <ul>
        @foreach (var item in List)
        {
            <li>@item</li>
        }
    </ul>
</div>

@code {
    [Parameter]
    public List<string> List { get; set; } = new();

    [Parameter]
    public string Title { get; set; } = string.Empty;
}